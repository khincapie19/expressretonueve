<!doctype html>

<html lang="en">
<head>
  <meta charset="utf-8">

  <title>The HTML5 Herald</title>
  <meta name="description" content="The HTML5 Herald">
  <meta name="author" content="SitePoint">

  <link rel="stylesheet" href="css/styles.css?v=1.0">
</head>

<body>

<table>
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Visits</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row"><%= `Id: ${Visitor._id}` %></th>
            <td><%= `name: ${Visitor.name}` %></td>
            <td><%= `Visits: ${Visitor.name}` %></td>
        </tr>
    </tbody>
</table>

</body>

</html>
